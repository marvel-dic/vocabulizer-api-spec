FORMAT: 1A

# Dictionary [/get-dictionary-from-src]

## Get a dictionary from src [GET]
Process text and return dictionary.

+ Request (application/json)

    + Body

            {
                "src": "Hello world"
            }

    + Schema

            {
                "type": "object",
                "properties": {
                    "src": { "type": "string" }
                },
                "additionalProperties": false
            }



+ Response 200 (application/json)

    + Body

            {
                "data": {
                    "src": "Hello world!",
                    "dictionary": [
                        {
                            "dictionaryWord": "hello",
                            "partOfSpeechTag": "INTJ",
                            "entriesInSrc": [{ "start": 0, "end": 5 }]
                        },
                        {
                            "dictionaryWord": "world",
                            "partOfSpeechTag": "NOUN",
                            "entriesInSrc": [{ "start": 6, "end": 11 }]
                        }
                    ]
                },
                "error": null,
                "meta": { "languages": ["en"] }
            }

    + Schema

            {
                "type": "object",
                "required": ["data", "error", "meta"],
                "properties": {
                    "data": {
                        "type": "object",
                        "required": ["src", "dictionary"],
                        "properties": {
                            "src": { "type": "string" },
                            "dictionary": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "required": ["dictionaryWord", "partOfSpeechTag", "entriesInSrc"],
                                    "properties": {
                                        "dictionaryWord": { "type": "string" },
                                        "partOfSpeechTag": { "type": "string" },
                                        "entriesInSrc": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "required": ["start", "end"],
                                                "properties": {
                                                    "start": { "type": "integer" },
                                                    "end": { "type": "integer" }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "error": { "type": "null" },
                    "meta": {
                        "type": "object",
                        "required": ["languages"],
                        "properties": {
                            "languages": { "type": "array", "items": { "type": "string" } }
                        }
                    }
                }
            }
